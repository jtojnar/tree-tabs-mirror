{
	"applications": {
		"gecko": {
			"id": "TreeTabs@jagiello.it"
		}
	},
	"manifest_version": 2,
	"default_locale": "en",
	"background": {
		"scripts": [ "background.js" ],
		"persistent": true
	},
	"name": "Tree Tabs",
	"description": "",
	"icons": {
		"128": "icons/128.png",
		"19": "icons/19.png",
		"38": "icons/38.png",
		"48": "icons/48.png"
	},
	"permissions": [ "tabs", "sessions", "chrome://favicon/" ],
	"options_page": "options.html",

	"options_ui": {
	  "page": "options.html"
	},

	"sidebar_action": {
		"default_icon": {
			"19": "icons/19.png",
			"38": "icons/38.png"
		},
		"default_panel": "sidebar.html",
		"default_title": "Tree Tabs"
	},
	"version": "0.0.8"
}
